{
  "name": "scs-action",
  "version": "1.0.6",
  "private": false,
  "author": "Artur Sharapov",
  "license": "MIT",
  "description": "Automate custom and preset scs files creation with GitHub Actions",
  "main": "lib/main.js",
  "assets": ["tempaltes", "path/to/other.js"],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qaip/scs-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@octokit/webhooks-definitions": "^3.67.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.15.10",
    "@typescript-eslint/parser": "^5.57.0",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.36.0",
    "eslint-plugin-github": "^4.7.0",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.5.0",
    "js-yaml": "^4.1.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.2"
  }
}
